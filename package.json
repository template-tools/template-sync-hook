{
  "name": "npm-template-sync-github-hook",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/npm-template-sync-github-hook.js",
  "module": "src/npm-template-sync-github-hook.js",
  "description": "github hook for npm-template-sync",
  "keywords": [
    "npm",
    "template",
    "webhook",
    "github"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "npm-template-sync-github-hook": "bin/npm-template-sync-github-hook"
  },
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "precover": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "start": "node bin/npm-template-sync-github-hook",
    "docs": "documentation readme src/npm-template-sync-github-hook.js --section=API",
    "semantic-release": "semantic-release",
    "prepare": "rollup -c",
    "lint": "documentation lint src/npm-template-sync-github-hook.js",
    "pkg": "rollup -c && pkg -t node8-linux bin/npm-template-sync-github-hook",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "github-repository-provider": "^3.0.0",
    "github-webhook-handler": "^0.7.0",
    "micro": "^9.3.2",
    "now-logs": "0.0.7",
    "npm-template-sync": "^7.4.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "documentation": "^8.1.2",
    "got": "^9.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^13.0.1",
    "rollup": "^0.65.0",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "semantic-release": "^15.9.12",
    "travis-deploy-once": "^5.0.4",
    "x-hub-signature": "^1.2.0",
    "rollup-plugin-cleanup": "^3.0.0"
  },
  "engines": {
    "node": ">=10.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-template-sync-github-hook.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/npm-template-sync-github-hook/issues"
  },
  "homepage": "https://github.com/arlac77/npm-template-sync-github-hook#readme",
  "release": {},
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
