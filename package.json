{
  "name": "npm-template-sync-github-hook",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/npm-template-sync-github-hook.mjs",
  "module": "src/npm-template-sync-github-hook.mjs",
  "description": "github hook for npm-template-sync",
  "keywords": [
    "npm",
    "template",
    "webhook",
    "github"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "npm-template-sync-github-hook": "bin/npm-template-sync-github-hook"
  },
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "test": "ava",
    "posttest": "npm run prepare && markdown-doctest",
    "start": "node bin/npm-template-sync-github-hook",
    "docs": "documentation readme src/npm-template-sync-github-hook.mjs --section=API",
    "semantic-release": "semantic-release",
    "prepare": "rollup -c",
    "lint": "documentation lint src/npm-template-sync-github-hook.mjs",
    "travis-deploy-once": "travis-deploy-once",
    "package": "npm-pkgbuild -i /services/npm-template-sync-github-hook"
  },
  "dependencies": {
    "github-repository-provider": "^5.1.13",
    "github-webhook-handler": "^0.7.0",
    "micro": "^9.3.3",
    "npm-template-sync": "^7.12.14",
    "sd-notify": "^2.5.1",
    "systemd": "^0.3.1"
  },
  "devDependencies": {
    "ava": "^1.0.1",
    "c8": "^3.2.1",
    "documentation": "^9.1.1",
    "esm": "^3.0.84",
    "got": "^9.5.0",
    "markdown-doctest": "^0.9.1",
    "npm-pkgbuild": "^1.3.0",
    "rollup": "^1.0.0",
    "rollup-plugin-cleanup": "^3.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "semantic-release": "^15.13.2",
    "travis-deploy-once": "^5.0.11",
    "x-hub-signature": "^1.2.0"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-template-sync-github-hook.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/npm-template-sync-github-hook/issues"
  },
  "homepage": "https://github.com/arlac77/npm-template-sync-github-hook#readme",
  "release": {},
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "pacman" : {
    "groups": "webhook",
    "backup": "systemd/npm-template-sync-bithub-hook.service",
    "install": "pacman.install"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
